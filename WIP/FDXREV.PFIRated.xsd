						<xs:annotation>
							<xs:documentation>xs:dateTime</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="tranCrctnDt" type="xs:dateTime">
						<xs:annotation>
							<xs:documentation>xs:dateTime</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="trnsmtDtTmStmp" type="xs:dateTime">
